      "source": [
        "# Usamos el siguiente link como referencia: https://scikit-learn.org/stable/auto_examples/model_selection/plot_learning_curve.html\n",
        "np.random.seed(37)\n",
        "arbol_optimo = DecisionTreeClassifier(max_depth= 7, criterion = 'gini', min_samples_split = 2, max_features= 6)\n",
        "svc_optimo = SVC( C = 3.9307142857142856)\n",
        "LDA_optimo = LinearDiscriminantAnalysis(tol = 0.16, solver = 'lsqr',shrinkage = 0.18)\n",
        "\n",
        "fig, ax = plt.subplots(nrows=1, ncols=3, figsize=(20, 6), sharey=True)\n",
        "\n",
        "common_params = {\n",
        "    \"X\": X_desarrollo,\n",
        "    \"y\": y_desarrollo,\n",
        "    \"train_sizes\": np.linspace(0.1, 1.0, 5),\n",
        "    \"cv\" : StratifiedKFold(5),\n",
        "    \"score_type\": \"both\",\n",
        "    \"n_jobs\": 4,\n",
        "    \"line_kw\": {\"marker\": \"o\"},\n",
        "    \"std_display_style\": \"fill_between\",\n",
        "    \"score_name\": \"Auc Roc\",\n",
        "    \"scoring\" : \"roc_auc\"\n",
        "}\n",
        "\n",
        "for ax_idx, estimator in enumerate([arbol_optimo,svc_optimo,LDA_optimo]):\n",
        "    LearningCurveDisplay.from_estimator(estimator, **common_params, ax=ax[ax_idx])\n",
        "    handles, label = ax[ax_idx].get_legend_handles_labels()\n",
        "    ax[ax_idx].legend(handles[:2], [\"Training Score\", \"Test Score\"])\n",
        "    ax[ax_idx].set_title(f\"Curva de aprendizaje para {estimator.__class__.__name__}\")"

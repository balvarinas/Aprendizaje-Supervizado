        "\n",
        "scores_train_arbol = []\n",
        "scores_test_arbol = []\n",
        "\n",
        "for depth in range(3, 15):\n",
        "  arbol_depth = DecisionTreeClassifier(max_depth = depth, criterion = 'gini', min_samples_split = 2, max_features= 6)\n",
        "\n",
        "  train, test = k_foldValidation(arbol_depth, X_desarrollo, y_desarrollo)\n",
        "  train_mean = sum(train[AUC_ROC])/len(train[AUC_ROC])\n",
        "  test_mean = sum(test[AUC_ROC])/len(test[AUC_ROC])\n",
        "\n",
        "  scores_train_arbol.append(train_mean)\n",
        "  scores_test_arbol.append(test_mean)"

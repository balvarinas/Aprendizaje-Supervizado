        "\n",
        "accuracy_global = np.asarray(test[ACCURACY]).max()\n",
        "AUPRC_global = np.asarray(test[PRECISION]).max()\n",
        "AUCROC_global = np.asarray(test[AUC_ROC]).max()\n",
        "\n",
        "score_table = pd.DataFrame({\n",
        "    \"permutación\": list(range(1,6)) + [\"Promedios\", \"Global\"],\n",
        "    \"Accuracy(training)\": list(np.asarray(train[ACCURACY]).round(4)) + [np.asarray(train[ACCURACY]).mean().round(4), \"(NO)\"],\n",
        "    \"Accuracy (validación)\": list(np.asarray(test[ACCURACY]).round(4)) + [np.asarray(test[ACCURACY]).mean().round(4), accuracy_global.round(4)],\n",
        "    \"AUPRC (training)\": list(np.asarray(train[ACCURACY]).round(4)) + [np.asarray(train[ACCURACY]).mean().round(4), \"(NO)\"],\n",
        "    \"AUPRC (validación)\": list(np.asarray(test[PRECISION]).round(4)) + [np.asarray(test[PRECISION]).mean().round(4), AUPRC_global.round(4)],\n",
        "    \"AUC ROC (training)\": list(np.asarray(train[AUC_ROC]).round(4)) + [np.asarray(train[AUC_ROC]).mean().round(4), \"(NO)\"],\n",
        "    \"AUC ROC (validación)\": list(np.asarray(test[AUC_ROC]).round(4)) + [np.asarray(test[AUC_ROC]).mean().round(4), AUCROC_global.round(4)]\n",
        "    })\n",
        "\n",
        "score_table"

        "scores_train_SVC = []\n",
        "scores_test_SVC = []\n",
        "\n",
        "regulation = np.linspace(0.7,3.7, num=30)\n",
        "\n",
        "for reg in regulation:\n",
        "  SVC_C = SVC(kernel = \"rbf\", gamma = 'scale',  C= reg)\n",
        "\n",
        "  SVC_CV = cross_validate(SVC_C , X_desarrollo, y_desarrollo, cv=5, scoring = \"roc_auc\", return_train_score= True)\n",
        "\n",
        "  SVC_score_train = SVC_CV[\"train_score\"]\n",
        "  SVC_score_test = SVC_CV[\"test_score\"]\n",
        "\n",
        "  SVC_score_train_mean = np.asarray(SVC_score_train).mean().round(4)\n",
        "  SVC_score_test_mean = np.asarray(SVC_score_test).mean().round(4)\n",
        "\n",
        "  scores_train_SVC.append(SVC_score_train_mean)\n",
        "  scores_test_SVC.append(SVC_score_test_mean)\n"

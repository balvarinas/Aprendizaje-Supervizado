      "source": [
        "## Implementación k-fold validation\n"
      ],
      "metadata": {
        "id": "e68UfWhx3HIQ"
      }
    },
    {
      "cell_type": "code",
      "source": [
        "np.random.seed(344)\n",
        "\n",
        "def k_foldValidation(clasificador, x_desarrollo: PandasVector, y_desarrollo: PandasVector) -> list[list]:\n",
        "\n",
        "  CONST_ACCURACY_INDEX  = 0\n",
        "  CONST_PRECISION_INDEX = 1\n",
        "  CONST_AUC_ROC_INDEX   = 2\n",
        "  CONST_K_ESCOGIDO      = 5\n",
        "\n",
        "\n",
        "  kf = KFold(n_splits = CONST_K_ESCOGIDO)\n",
        "  y_pred_test = np.empty(y_desarrollo.shape)\n",
        "  y_pred_test.fill(np.nan)\n",
        "\n",
        "  y_pred_train = np.empty(y_desarrollo.shape)\n",
        "  y_pred_train.fill(np.nan)\n",
        "\n",
        "  scores_train = [[] for _ in range(3)]\n",
        "  scores_test = [[] for _ in range(3)]\n",
        "\n",
        "\n",
        "  for train_index, test_index in kf.split(x_desarrollo):\n",
        "    x_train, x_test = x_desarrollo.iloc[train_index], x_desarrollo.iloc[test_index]\n",
        "    y_train, y_test = y_desarrollo.iloc[train_index], y_desarrollo.iloc[test_index]\n",
        "\n",
        "    clasificador_trained = clasificador.fit(x_train, y_train)\n",
        "\n",
        "    predictions_test = clasificador_trained.predict(x_test)\n",
        "    predictions_train = clasificador_trained.predict(x_train)\n",
        "\n",
        "    y_pred_test[test_index] = predictions_test\n",
        "    y_pred_train[train_index] = predictions_train\n",
        "\n",
        "    y_pred_test_current_fold = []\n",
        "    for index in test_index: y_pred_test_current_fold.append(y_pred_test[index])\n",
        "\n",
        "    y_pred_train_current_fold = []\n",
        "    for index in train_index: y_pred_train_current_fold.append(y_pred_train[index])\n",
        "\n",
        "    accuracy_test = accuracy_score(y_pred_test_current_fold, y_test)\n",
        "    accuracy_train = accuracy_score(y_pred_train_current_fold, y_train)\n",
        "\n",
        "    average_precision_test = average_precision_score(y_pred_test_current_fold, y_test)\n",
        "    average_precision_train = average_precision_score(y_pred_train_current_fold, y_train)\n",
        "\n",
        "    roc_auc_test = roc_auc_score(y_pred_test_current_fold, y_test)\n",
        "    roc_auc_train = roc_auc_score(y_pred_train_current_fold, y_train)\n",
        "\n",
        "    scores_train[CONST_ACCURACY_INDEX].append(accuracy_train)\n",
        "    scores_train[CONST_PRECISION_INDEX].append(average_precision_train)\n",
        "    scores_train[CONST_AUC_ROC_INDEX].append(roc_auc_train)\n",
        "\n",
        "    scores_test[CONST_ACCURACY_INDEX].append(accuracy_test)\n",
        "    scores_test[CONST_PRECISION_INDEX].append(average_precision_test)\n",
        "    scores_test[CONST_AUC_ROC_INDEX].append(roc_auc_test)\n",
        "\n",
        "  return scores_train, scores_test\n"
      ],
      "metadata": {
        "id": "CRHckw21zsYl"
